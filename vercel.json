{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/annotation",
      "destination": "https://applications-khaki.vercel.app/annotation"
    },
    {
      "source": "/annotation/",
      "destination": "https://applications-khaki.vercel.app/annotation/"
    },
    {
      "source": "/annotation/:path*",
      "destination": "https://applications-khaki.vercel.app/annotation/:path*"
    },
    {
      "source": "/annotation/:path*/",
      "destination": "https://applications-khaki.vercel.app/annotation/:path*/"
    },
    {
      "source": "/notes",
      "destination": "https://notes-jade-eight.vercel.app/notes"
    },
    {
      "source": "/notes/",
      "destination": "https://notes-jade-eight.vercel.app/notes/"
    },
    {
      "source": "/notes/:path*",
      "destination": "https://notes-jade-eight.vercel.app/notes/:path*"
    },
    {
      "source": "/notes/:path*/",
      "destination": "https://notes-jade-eight.vercel.app/notes/:path*/"
    }
  ]
}